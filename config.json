{
    "listeners": [
        {
            "address": "0.0.0.0",
            "port": 8080,
            "https": false
        }
    ],
    "db_clients": [
        {
            "name": "default",
            "rdbms": "postgresql",
            "host": "127.0.0.1",
            "port": 5432,
            "dbname": "starlighterp_db",
            "user": "starlighterp_api",
            "passwd": "StarLightERP_API_01",
            "is_fast": false,
            "number_of_connections": 1,
            "timeout": -1.0
        }
    ],
    "app": {
        "number_of_threads": 1,
        "enable_session": false,
        "document_root": "./",
        "home_page": "index.html",
        "use_implicit_page": true,
        "upload_path": "uploads",
        "file_types": [
            "gif",
            "png",
            "jpg",
            "js",
            "css",
            "html",
            "ico",
            "swf",
            "xap",
            "apk",
            "cur",
            "xml",
            "webp",
            "svg"
        ],
        "max_connections": 100000,
        "max_connections_per_ip": 0,
        "load_dynamic_views": false,
        "dynamic_views_path": [
            "./views"
        ],
        "log": {
            "use_spdlog": false,
            "log_level": "DEBUG"
        },
        "run_as_daemon": false,
        "handle_sig_term": true,
        "relaunch_on_error": false,
        "use_sendfile": true,
        "use_gzip": true,
        "static_files_cache_time": 5,
        "idle_connection_timeout": 60,
        "server_header_field": "",
        "enable_server_header": true,
        "enable_date_header": true,
        "keepalive_requests": 0,
        "pipelining_requests": 0,
        "gzip_static": true,
        "br_static": true,
        "client_max_body_size": "1M",
        "client_max_memory_body_size": "64K",
        "client_max_websocket_message_size": "128K"
    },
    "filters": [
        {
            "name": "TenantMiddleware",
            "path": "/api/test/*"
        }
    ]
}